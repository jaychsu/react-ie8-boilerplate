{
  "name": "react-ie8-boilerplate",
  "version": "0.0.1",
  "description": "Quickly build web app via React and related tech.",
  "engines": {
    "node": ">=4.6.2",
    "npm": ">=2.15.11"
  },
  "scripts": {
    "start": "cross-env ENV_NAME=dev node node_modules/webpack-dev-server/bin/webpack-dev-server --config config/webpack.dev.js",
    "dev": "npm run start",
    "build": "cross-env ENV_NAME=prod node node_modules/webpack/bin/webpack --config config/webpack.prod.js",
    "preprod": "jest",
    "prod": "npm run build",
    "test": "jest --coverage"
  },
  "license": "MIT",
  "author": "Jaych Su",
